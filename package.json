        "dotenv": "^8.1.0",
        "ejs": "^2.6.2",
        "express": "~4.16.0",
        "express-basic-auth": "^1.2.0",
        "fabric-ca-client": "^2.2.11",
        "fabric-network": "^2.2.11",
        "http-errors": "~1.6.2",
        "joi": "^17.4.0",
        "moment": "^2.24.0",
